<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">true</item>
        
        <!-- Enhanced theme attributes for better consistency -->
        <item name="android:colorBackground">@color/background</item>
        <item name="colorSurface">@color/surface</item>
        <item name="colorOnBackground">@color/text_primary</item>
        <item name="colorOnSurface">@color/text_primary</item>
        <item name="android:textColorPrimary">@color/text_primary</item>
        <item name="android:textColorSecondary">@color/text_secondary</item>
        
        <!-- Material component overrides -->
        <item name="materialButtonStyle">@style/Widget.App.Button</item>
        <item name="cardViewStyle">@style/Widget.App.CardView</item>
    </style>

    <style name="Theme.QwinAI" parent="Theme.MaterialComponents.Light">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/colorPrimary</item>
        <item name="android:navigationBarColor">@color/colorPrimary</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
    </style>

    <!-- QwinAI Theme without Action Bar (for activities that use custom toolbars) -->
    <style name="Theme.QwinAI.NoActionBar" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/colorPrimary</item>
        <item name="android:navigationBarColor">@color/colorPrimary</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!-- Alternative: You can also extend your existing AppTheme -->
    <style name="Theme.QwinAI.Alternative" parent="AppTheme">
        <!-- This uses your existing AppTheme as base -->
    </style>
    <style name="ThinkingTextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">monospace</item>
        <item name="android:textColor">@color/thinking_text_color</item>
        <item name="android:background">@drawable/thinking_text_background</item>
        <item name="android:padding">8dp</item>
    </style>
    <!-- If you need a splash screen theme -->

    <style name="CustomTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">sans-serif-medium</item>
    </style>

    <style name="TabTextStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">@font/rubik_medium</item>
    </style>
    <!-- Enhanced Card Styles -->
    <style name="Widget.App.CardView" parent="Widget.MaterialComponents.CardView">
        <item name="cardElevation">4dp</item>
        <item name="cardCornerRadius">20dp</item>
        <item name="strokeWidth">0.5dp</item>
        <item name="strokeColor">@color/card_stroke</item>
        <item name="cardPreventCornerOverlap">true</item>
        <item name="cardUseCompatPadding">true</item>
        <item name="rippleColor">@color/ripple_material_light</item>
        <item name="cardBackgroundColor">@color/card_background</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.Card.Rounded" parent="">
        <item name="cornerSize">16dp</item>
        <item name="cornerFamily">rounded</item>
    </style>
    <style name="Widget.App.CardView.Elevated" parent="Widget.App.CardView">
        <item name="cardElevation">8dp</item>
        <item name="strokeWidth">0dp</item>
    </style>

    <style name="Widget.App.CardView.Premium" parent="Widget.App.CardView">
        <item name="cardElevation">6dp</item>
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">@color/colorPrimary</item>
    </style>
    
    <!-- Enhanced Button Style -->
    <style name="Widget.App.Button" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@color/colorPrimary</item>
        <item name="android:textColor">@color/white</item>
        <item name="rippleColor">@color/colorPrimaryDark</item>
    </style>

    <!-- AppBar and Popup overlay styles -->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <!-- =============================================================== -->
    <!-- BASE THEME - USED AS PARENT FOR ALL OTHER THEMES                -->
    <!-- =============================================================== -->


    <!-- Other original styles remain the same -->
    <style name="ShapeAppearanceOverlay.TopRounded" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSizeTopLeft">24dp</item>
        <item name="cornerSizeTopRight">24dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

    <style name="TabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/tab_icon_color</item>
        <item name="textAllCaps">false</item>
        <item name="fontFamily">sans-serif-medium</item>
    </style>

    <style name="SpinnerDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
    </style>

    <style name="ShapeAppearance.App.StartRounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">8dp</item>
        <item name="cornerSizeBottomLeft">8dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

    <style name="EditMessageDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
        <item name="android:windowAnimationStyle">@style/EditMessageDialogAnimation</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowElevation">0dp</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.3</item>
    </style>

    <style name="EditMessageDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>
    </style>

    <style name="ToolbarTitle">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

